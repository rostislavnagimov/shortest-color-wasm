{
  "name": "shortest-color",
  "version": "0.1.3",
  "description": "Fast shortest CSS color converter (Rust+WASM)",
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.mjs"
    }
  },
  "files": [
    "index.cjs",
    "index.mjs",
    "index.d.ts",
    "node.js",
    "node_bg.wasm",
    "shortest_color_wasm.js",
    "shortest_color_wasm_bg.js",
    "shortest_color_wasm_bg.wasm"
  ],
  "scripts": {
    "clean": "rm -rf pkg-node pkg-bundler index.* node.* shortest_color_wasm*",
    "build:node": "wasm-pack build --release --target nodejs --out-dir pkg-node",
    "build:bundler": "wasm-pack build --release --target bundler --out-dir pkg-bundler",
    "build": "npm run build:node && npm run build:bundler && node create-wrapper.cjs",
    "prepublishOnly": "npm run build"
  }
}