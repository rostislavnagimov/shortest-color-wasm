{
  "name": "shortest-color",
  "version": "0.1.5",
  "description": "Fast shortest CSS color converter (Rust+WASM)",
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
    "keywords": [
    "color",
    "css",
    "parser",
    "hex",
    "rgb",
    "rgba",
    "hsl",
    "hsla",
    "shorten",
    "converter",
    "webcolors",
    "shortest-color"
  ],
  "author": "Rostislav Nagimov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rostislavnagimov/shortest-color-wasm"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.mjs"
    }
  },
  "files": [
    "index.cjs",
    "index.mjs",
    "index.d.ts",
    "node.js",
    "node_bg.wasm",
    "shortest_color_wasm.js",
    "shortest_color_wasm_bg.js",
    "shortest_color_wasm_bg.wasm"
  ],
  "scripts": {
    "clean": "rm -rf pkg-node pkg-bundler index.* node.* shortest_color_wasm*",
    "build:node": "wasm-pack build --release --target nodejs --out-dir pkg-node",
    "build:bundler": "wasm-pack build --release --target bundler --out-dir pkg-bundler",
    "build": "npm run build:node && npm run build:bundler && node create-wrapper.cjs",
    "prepublishOnly": "npm run build"
  }
}